<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent">

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/layout_margin_right"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="0dp" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/layout_margin_right"
        tools:layout_editor_absoluteX="395dp"
        tools:layout_editor_absoluteY="0dp" />

    <Button
        android:id="@+id/register"
        android:layout_width="338dp"
        android:layout_height="54dp"
        android:layout_gravity="start"
        android:layout_marginLeft="26dp"
        android:layout_marginRight="26dp"
        android:layout_marginBottom="139dp"
        android:backgroundTint="@color/teal_700"
        android:enabled="false"
        android:text="Подать заявку"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <Button
        android:id="@+id/login"
        android:layout_width="338dp"
        android:layout_height="54dp"
        android:layout_gravity="start"
        android:layout_marginLeft="26dp"
        android:layout_marginRight="26dp"
        android:layout_marginBottom="79dp"
        android:backgroundTint="@color/black"
        android:enabled="false"
        android:text="Изменить данные в профиле"
        android:textColor="@color/white"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.497"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/bitmap__1_"
        tools:layout_editor_absoluteX="29dp"
        tools:layout_editor_absoluteY="267dp" />

    <ImageView
        android:id="@+id/imageView11"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:srcCompat="@drawable/bitmap"
        tools:layout_editor_absoluteX="34dp"
        tools:layout_editor_absoluteY="402dp" />

    <TextView
        android:id="@+id/answerfound"
        android:layout_width="250dp"
        android:layout_height="100dp"
        android:text="Быстрое прохождение регистрации, потребуется ответить только на часть вопроов"
        android:textColor="#999999"
        android:textSize="14dp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="315dp" />

    <TextView
        android:id="@+id/answer"
        android:layout_width="230dp"
        android:layout_height="150dp"
        android:text="Данные будут автозаполнены из профиля"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="267dp" />

    <TextView
        android:id="@+id/answer2"
        android:layout_width="230dp"
        android:layout_height="150dp"
        android:text="Всё под защитой"
        android:textColor="#000000"
        android:textSize="16dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="402dp" />

    <TextView
        android:id="@+id/answerfound"
        android:layout_width="250dp"
        android:layout_height="100dp"
        android:text="Информация передается, обрабатывается и хранится в соответствии с законодательством РФ"
        android:textColor="#999999"
        android:textSize="14dp"
        tools:layout_editor_absoluteX="85dp"
        tools:layout_editor_absoluteY="430dp" />

    <TextView
        android:id="@+id/textView2"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:width="250dp"
        android:height="100dp"
        android:text="Подача анкеты"
        android:textAlignment="center"
        android:textColor="@color/black"
        android:textSize="34dp"
        android:textStyle="bold"
        tools:layout_editor_absoluteX="73dp"
        tools:layout_editor_absoluteY="166dp" />

</androidx.constraintlayout.widget.ConstraintLayout>